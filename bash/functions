function c {
  cd ~/code/$1
}

function __colorize_git_ps1 () {
  local branch=$(__git_ps1 "%s")
  if [ -n "$branch" ]; then
    local color="33"
    if [[ $branch == master* ]]; then
      color="31" #Red
    fi
    if [[ $branch == dev* ]]; then
      color="32" #Green
    fi
    if [[ $branch == features* ]]; then
      color="36" #Blue
    fi
    if [[ $branch == fix* ]]; then
      color="35" #Light Red
    fi

    printf "[\e[${color}m${branch}\e[0m]"
  fi
}
